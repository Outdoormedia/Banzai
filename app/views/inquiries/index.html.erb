<h1>Listing inquiries</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Title</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Address</th>
    <th>Interest</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @inquiries.each do |inquiry| %>
  <tr>
    <td><%= inquiry.name %></td>
    <td><%= inquiry.title %></td>
    <td><%= inquiry.phone %></td>
    <td><%= inquiry.email %></td>
    <td><%= inquiry.address %></td>
    <td><%= inquiry.interest %></td>
    <td><%= link_to 'Show', inquiry %></td>
    <td><%= link_to 'Edit', edit_inquiry_path(inquiry) %></td>
    <td><%= link_to 'Destroy', inquiry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Inquiry', new_inquiry_path %>
